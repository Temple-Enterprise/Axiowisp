{
  "name": "axiowisp",
  "version": "0.3.2",
  "private": true,
  "description": "Axiowisp â€” a modern desktop IDE built with Electron, React & Monaco",
  "author": "Temple Enterprise LLC",
  "main": "dist-electron/electron/main.js",
  "scripts": {
    "dev": "concurrently -k \"vite\" \"npm run dev:electron\"",
    "dev:electron": "tsc -p tsconfig.electron.json && electron .",
    "dev:vite": "vite",
    "build:renderer": "tsc && vite build",
    "build:electron": "tsc -p tsconfig.electron.json",
    "build": "npm run build:renderer && npm run build:electron",
    "preview": "vite preview",
    "package": "npm run build && electron-builder --win"
  },
  "build": {
    "appId": "com.axiowisp.app",
    "productName": "Axiowisp",
    "copyright": "0.3.2 | Temple Enterprise LLC | 2026",
    "artifactName": "Axiowisp.${ext}",
    "icon": "public/icon-bg.ico",
    "npmRebuild": false,
    "directories": {
      "output": "release"
    },
    "files": [
      "dist-electron/**/*",
      "dist/**/*"
    ],
    "win": {
      "target": "nsis",
      "icon": "public/icon-bg.ico",
      "verifyUpdateCodeSignature": false,
      "signAndEditExecutable": false
    },
    "nsis": {
      "oneClick": false,
      "perMachine": false,
      "allowToChangeInstallationDirectory": true,
      "installerIcon": "public/icon-bg.ico",
      "uninstallerIcon": "public/icon-bg.ico",
      "createDesktopShortcut": true,
      "createStartMenuShortcut": true,
      "shortcutName": "Axiowisp"
    },
    "fileAssociations": [
      {
        "ext": "ts",
        "name": "TypeScript",
        "role": "Editor"
      },
